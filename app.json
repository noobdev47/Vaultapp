{
  "expo": {
    "name": "Vaultapp",
    "description": "An App for storing account credentials securely",
    "slug": "Vaultapp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/VaultappIcon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ignite.Vaultapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
